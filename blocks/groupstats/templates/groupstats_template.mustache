<div class="group-stats" id="group-stats" style="overflow-x: auto;overflow-y: hidden;">

    <div class="gst-tabs">
        <div class="gst-tab active" id="GroupPerformanceTab">
            Успеваемость
        </div>
        <!--div class="gst-tab" id="GroupAttendanceTab">
            Посещаемость
        </div-->
        <div class="gst-tab" id="GroupPaymentTab">
            Оплата
        </div>
    </div>

    <div class="gst-tabs-contents show" id="group-performance">

        <div class="gst-pie-diagram">
            {{#performanceEmptyImageURL}}
                <img width="100%" height="100%" src="{{.}}" />
            {{/performanceEmptyImageURL}}

            {{^performanceEmptyImageURL}}
                <canvas id="gst-pie-diagram-performance" width="100%" height="100%"></canvas>
            {{/performanceEmptyImageURL}}
        </div>

        <div class="group-performance" style="display: flex;">
            <div style="flex: auto; max-width: 300px; margin-right: 20px;">
                <div class="gst-line-bold">
                    <div class="gst-line-text">
                        Занятий пройдено
                    </div>
                    <div class="gst-line-value">
                        12
                    </div>
                </div>
                <div class="gst-line" id="well-attending-students">
                    <div class="gst-line-text">
                        Успеваемость
                    </div>
                    <div class="gst-line-value">
                        {{#performance}}
                            <div>{{.}}%</div>
                        {{/performance}}
                        {{^performance}}
                            <div>N/A</div>
                        {{/performance}}
                        <!--div>{{groupmemberscount}}</div-->
                    </div>
                </div>
                <div class="gst-line" id="well-attending-students">
                    <div class="gst-line-text">
                        Качество знаний
                    </div>
                    <div class="gst-line-value">
                        {{#kquality}}
                            <div>{{.}}%</div>
                        {{/kquality}}
                        {{^kquality}}
                            <div>N/A</div>
                        {{/kquality}}
                    </div>
                </div>
                <div class="gst-line" id="well-attending-students">
                    <div class="gst-line-text">
                        Обученность
                    </div>
                    <div class="gst-line-value">
                        {{#learning}}
                            <div>{{.}}%</div>
                        {{/learning}}
                        {{^learning}}
                            <div>N/A</div>
                        {{/learning}}
                        <!--div>{{groupmemberscount}}</div-->
                    </div>
                </div>


            </div>

            <div style="margin-right: 15px; display: none">
                <div class="gst-line-bold">
                    <div class="gst-line-text">
                        Занятий пройдено
                    </div>
                    <div class="gst-line-value">
                        12
                    </div>
                </div>
                <div class="gst-line active" id="well-students">
                    <div class="diagram-sample-color gst-color-well"></div>
                    <div class="gst-line-text">
                        Отличники
                    </div>
                    <div class="gst-line-value">
                        <div id="gs-wellstudentscount">{{wellstudentscount}}</div>
                        /
                        <div>{{groupmemberscount}}</div>
                    </div>
                </div>
                <div class="gst-line" id="good-students">
                    <div class="diagram-sample-color gst-color-good"></div>
                    <div class="gst-line-text">
                        Хорошисты
                    </div>
                    <div class="gst-line-value">
                        <div id="gs-goodstudentscount">{{goodstudentscount}}</div>
                        /
                        <div>{{groupmemberscount}}</div>
                    </div>
                </div>
                <div class="gst-line" id="okay-students">
                    <div class="diagram-sample-color gst-color-okay"></div>
                    <div class="gst-line-text">
                        Успевающие
                    </div>
                    <div class="gst-line-value">
                        <div id="gs-okaystudentscount">{{okaystudentscount}}</div>
                        /
                        <div>{{groupmemberscount}}</div>
                    </div>
                </div>
                <div class="gst-line" id="bad-students">
                    <div class="diagram-sample-color gst-color-bad"></div>
                    <div class="gst-line-text">
                        Неуспевающие
                    </div>
                    <div class="gst-line-value">
                        <div id="gs-badstudentscount">{{badstudentscount}}</div>
                        /
                        <div>{{groupmemberscount}}</div>
                    </div>
                </div>
            </div>
            <div class="gst-list-holder">


                <div class="gst-line-bold"
                    style="display: flex; flex-wrap: nowrap; align-items: center; justify-content: space-around;">
